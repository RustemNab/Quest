<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.questqazan.androidprojectv2">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Quest Qazan"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".TasksActivity" />
        <activity android:name=".RegistrActivity" />
        <activity android:name=".InterfaceActivity" />
        <activity android:name=".GoToGameActivity" />
        <activity android:name=".GameInterfaceActivity" />
        <activity android:name=".QuestionsActivity" />
        <activity android:name=".QuestionAnswerActivity" />
        <activity android:name=".TaskAnswerActivity"></activity>
    </application>

</manifest>